[user]
    name = Michael Carroll
    email = carroll.michael@gmail.com
[core]
    excludesfile = ~/.gitignore_global
    pager = cat
    editor = vim
[alias]
    gl = log -10 --color=always --all --graph --topo-order --pretty='format: %Cgreen%h%Creset %s%Cred%d%Creset%n'
    gll = log --color=always --all --graph --topo-order --pretty='format: %Cgreen%h%Creset %s%Cred%d%Creset%n'
    com = commit
    cm = commit -m
    co = checkout
    stat = status
    st = status
    unstage = reset HEAD
    uns = reset HEAD
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[diff]
    tool = vimdiff
